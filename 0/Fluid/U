{

	Interface
	{
		type	noSlip;
	}

	Inlet
	{
		type	surfaceNormalFixedValue;
		refValue	uniform -0.1;
	}
	Outlet
	{
		type	pressureInletOutletVelocity;
		value	uniform (0.0 0.0 0.0);
	}
}
