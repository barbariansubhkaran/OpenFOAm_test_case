		absorptivity	1.0;
		transmissivity	0.0;
	}

	Inlet
	{
		type	boundaryRadiation;
		value	uniform 0;
		mode	lookup;
		emissivity	1.0;
		absorptivity	1.0;
		transmissivity	0.0;
	}
	Outlet
	{
		type	boundaryRadiation;
		value	uniform 0;
		mode	lookup;
		emissivity	1.0;
		absorptivity	1.0;
		transmissivity	0.0;
	}
}
