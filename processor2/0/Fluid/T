FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/Fluid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    Inlet
    {
        type            totalTemperature;
        gamma           1.4;
        T0              uniform 300;
        value           uniform 300;
    }
    Interface
    {
        type            compressible::turbulentTemperatureRadCoupledMixed;
        refValue        uniform 300;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        source          uniform 0;
        value           uniform 300;
        Tnbr            T;
        qrNbr           none;
        qr              qr;
        thermalInertia  false;
        kappaMethod     fluidThermo;
        kappa           none;
        field           T;
    }
    Outlet
    {
        type            inletOutlet;
        inletValue      uniform 300;
        value           uniform 300;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 300;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 300;
    }
}


