FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/Fluid";
    object      boundaryRadiationProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Inlet
    {
        type            boundaryRadiation;
        mode            lookup;
        emissivity      1;
        absorptivity    1;
        transmissivity  0;
        value           uniform 0;
    }
    Interface
    {
        type            boundaryRadiation;
        mode            lookup;
        emissivity      1;
        absorptivity    1;
        transmissivity  0;
        value           uniform 0;
    }
    Outlet
    {
        type            boundaryRadiation;
        mode            lookup;
        emissivity      1;
        absorptivity    1;
        transmissivity  0;
        value           uniform 0;
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
}


