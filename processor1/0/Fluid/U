FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0/Fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    Inlet
    {
        type            surfaceNormalFixedValue;
        refValue        uniform -0.1;
    }
    Interface
    {
        type            noSlip;
    }
    Outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


