FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0/Fluid";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    Inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.05;
        value           uniform 0.01;
    }
    Interface
    {
        type            kqRWallFunction;
        value           uniform 0.01;
    }
    Outlet
    {
        type            zeroGradient;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.01;
    }
    procBoundary1to2
    {
        type            processor;
        value           uniform 0.01;
    }
    procBoundary1to3
    {
        type            processor;
        value           uniform 0.01;
    }
}


