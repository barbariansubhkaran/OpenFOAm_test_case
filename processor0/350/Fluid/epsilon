FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "350/Fluid";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    Inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.001;
        phi             phi;
        k               k;
        value           uniform 1;
    }
    Interface
    {
        lowReCorrection 0;
        blending        stepwise;
        n               2;
        type            epsilonWallFunction;
        value           uniform 1;
    }
    Outlet
    {
        type            zeroGradient;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 1;
    }
}


