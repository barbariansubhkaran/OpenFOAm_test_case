{
    Inlet
    {
        type            calculated;
        value           uniform 0;
    }
    Interface
    {
        type            greyDiffusiveRadiationViewFactor;
        value           uniform 0;
        qro             uniform 0;
    }
    Outlet
    {
        type            calculated;
        value           uniform 0;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 0;
    }
}


